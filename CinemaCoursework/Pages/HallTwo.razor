@page "/hall_two"
@using CinemaCoursework.Data
@inject PagesList pageList

<h3>Hall two</h3>

@code {
    protected override async Task OnInitializedAsync()
    {
        await GetPageName();
    }

    public async Task GetPageName()
    {
        pageList.PageName = "HallTwo";
        await base.OnInitializedAsync();
    }
}