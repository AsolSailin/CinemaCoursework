@page "/admin_account"
@using CinemaCoursework.Data
@inject PagesList pageList

<h3>Admin account</h3>

@code {
    protected override async Task OnInitializedAsync()
    {
        await GetPageName();
    }

    public async Task GetPageName()
    {
        pageList.PageName = "AdminAccount";
        await base.OnInitializedAsync();
    }
}